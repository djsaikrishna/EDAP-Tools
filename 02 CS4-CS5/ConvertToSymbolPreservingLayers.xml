<dialog buttons="accept, cancel" title="Convert to Symbol ( Preserving Layers )">
	<grid>
		<columns>
			<column />
			<column />
		</columns>
		<rows>
			<row>
				<label value="Name:" />
				<textbox id="name" size="40" />
			</row>
			<row align="start">
				<label value="Type:               " />
				<radiogroup id="SymbolType">
					<radio label="Graphic"/>
					<radio label="Movie Clip" selected="true"/>
					<radio label="Button"/>
				</radiogroup>
			</row>
		</rows>
	</grid>
	<spacer></spacer>
	<separator></separator>
	<spacer></spacer>
</dialog>